{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": [
    "vue",
    "only-warn"
  ],
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "semi": "off",
    "no-multiple-empty-lines": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "vars-on-top": "off",
    "prefer-const": "off",
    "eol-last": "off",
    "no-param-reassign": "off",
    "space-before-function-paren": "off",
    "func-names": "off",
    "func-style": [
      "warn",
      "declaration"
    ],
    "no-use-before-define": [
      "warn",
      {
        "functions": false
      }
    ]
  }
}